{
  "name": "payload-tailwind-blog-starter",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"${NODE_OPTIONS} --no-deprecation\" next dev",
    "devturbo": "pnpm dev --turbo",
    "devsafe": "rm -rf .next && pnpm dev",
    "build": "pnpm payload migrate && pnpm generate:types && cross-env NODE_OPTIONS=--no-deprecation next build",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "ci": "payload migrate && pnpm build",
    "payload": "payload",
    "generate:types": "payload generate:types",
    "standalone-script": "payload run ./src/scripts/standalone-script.ts",
    "migrate:create": "payload migrate:create",
    "generate:importmap": "payload generate:importmap"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.0.0-beta.123",
    "@payloadcms/next": "3.0.0-beta.123",
    "@payloadcms/richtext-lexical": "3.0.0-beta.123",
    "@payloadcms/richtext-slate": "3.0.0-beta.123",
    "@payloadcms/storage-vercel-blob": "3.0.0-beta.123",
    "@payloadcms/ui": "3.0.0-beta.123",
    "@tailwindcss/typography": "^0.5.15",
    "@vercel/analytics": "^1.3.2",
    "autoprefixer": "^10.4.20",
    "babel-plugin-react-compiler": "0.0.0-experimental-48eb8f4-20240822",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "copy-to-clipboard": "^3.3.3",
    "cross-env": "^7.0.3",
    "next": "15.0.0",
    "payload": "3.0.0-beta.123",
    "payload-types": "link:@\\payload-types",
    "react": "19.0.0-rc-65a56d0e-20241020",
    "react-dom": "19.0.0-rc-65a56d0e-20241020",
    "sharp": "0.32.6",
    "slugify": "^1.6.6",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20.17.5",
    "@types/react": "npm:types-react@19.0.0-rc.0",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.0",
    "react-syntax-highlighter": "^15.6.1",
    "tailwindcss": "^3.4.14",
    "typescript": "5.5.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "19.0.0-rc-65a56d0e-20241020",
        "react-dom": "19.0.0-rc-65a56d0e-20241020",
        "scheduler": "0.0.0-experimental-3edc000d-20240926"
      },
      "ignoreMissing": [
        "react",
        "react-dom",
        "@types/react",
        "scheduler"
      ]
    }
  },
  "overrides": {
    "@types/react": "npm:types-react@19.0.0-rc.0",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0"
  }
}
